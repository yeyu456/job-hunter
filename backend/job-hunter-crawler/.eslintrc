{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "node": true,

        "es6":true
    },
    "rules": {

        "no-mixed-spaces-and-tabs": 2,

        "indent":[2, 4],

        "eqeqeq": 2,

        "strict":[2, "global"],

        "quotes": [
            2,
            "single",
            "avoid-escape"
        ],

        "no-var":2
    }
}
